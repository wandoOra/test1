{
  "expo": {
    "name": "wcashless for Business",
    "slug": "wcashless-for-business",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "react-native-nfc-manager",
      "expo-av"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wandoora.wcashless"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "NFC",
        "NFC_TRANSACTION_EVENT",
        "android.permission.NFC"
      ],
      "package": "com.wandoora.wcashless"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
